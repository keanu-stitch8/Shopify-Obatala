<div class="bg-white">
  <div class="max-w-8xl mx-auto px-4 lg:px-8">
    <h2 class="font-bold text-2xl text-center mb-4 lg:text-3xl">{{ section.settings.homepage_seo_text_title }}</h2>
    <div class="text-center space-y-6">{{ section.settings.homepage_seo_text_text }}</div>
  </div>
</div>

{% schema %}
  {
    "name": "SEO Text",
    "settings": [
      {
        "type": "text",
        "id": "homepage_seo_text_title",
        "label": "Input title",
        "default": "Title"
      },
      {
        "type": "richtext",
        "id": "homepage_seo_text_text",
        "label": "Input text",
        "default": "<p>Text</p>"
      }
    ]
  }
{% endschema %}